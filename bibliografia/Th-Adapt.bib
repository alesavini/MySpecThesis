issn = {01678140},
journal = {Radiotherapy and Oncology},
keywords = {Adaptive radiotherapy,Bladder cancer,Dose escalation,Plan of the day,Tomotherapy},
number = {1},
pages = {55--60},
pmid = {21397964},
publisher = {Elsevier Ireland Ltd},
title = {{'Plan of the day' adaptive radiotherapy for bladder cancer using helical tomotherapy}},
