number = {3},
pages = {986--993},
pmid = {22138459},
publisher = {Elsevier Inc},
title = {{Adaptive Radiotherapy for Head-and-Neck Cancer: Initial Clinical Outcomes from a Prospective Trial.}},
url = {http://dx.doi.org/10.1016/j.ijrobp.2011.08.017},
volume = {83},
year = {2011}
