eprint = {NIHMS150003},
file = {:C$\backslash$:/Users/Alessandro/Documents/Mendeley Desktop/2011 - Schwartz et al. - Adaptive Radiotherapy for Head-and-Neck Cancer Initial Clinical Outcomes from a Prospective Trial.pdf:pdf},
isbn = {0360-3016},
